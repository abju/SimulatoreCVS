<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.BorderPane" id="main" prefHeight="760.0" prefWidth="720.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <center>
    <SplitPane id="splitPane" fx:id="divisore" dividerPositions="0.5" focusTraversable="true" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
      <items>
        <TilePane fx:id="elenco_battute" alignment="TOP_LEFT" cache="false" depthTest="INHERIT" hgap="0.0" mouseTransparent="false" orientation="HORIZONTAL" prefHeight="200.0" prefWidth="200.0" vgap="5.0">
          <children>
            <TextField fx:id="nuovaBattuta" maxWidth="100.0" onKeyPressed="#nuovaBattutaKeyPress" onKeyReleased="#nuovaBattutaKeyReleased" prefWidth="100.0" />
          </children>
          <padding>
            <Insets bottom="0.0" left="5.0" right="5.0" top="15.0" />
          </padding>
        </TilePane>
        <BorderPane fx:id="borderBottom" prefHeight="200.0" prefWidth="200.0">
          <center>
            <csvsimulator.spartito.controller.SpartitoPentagrammaController fx:id="spartitoPentagramma"/>
          </center>
        </BorderPane>
      </items>
    </SplitPane>
  </center>
  <stylesheets>
    <URL value="@../css/spartitobase.css" />
  </stylesheets>
</fx:root>
