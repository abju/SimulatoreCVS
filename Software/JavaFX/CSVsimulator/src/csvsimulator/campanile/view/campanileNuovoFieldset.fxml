<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="javafx.scene.layout.HBox" id="main" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="27.0" prefWidth="900.0" spacing="15.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Label fx:id="lblNumeroCampana" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" text="" />
    <Label text="Nome:" />
    <TextField fx:id="tfNomeCampana" prefWidth="50.0" text="" />
    <Label fx:id="lblPathFile" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="400.0" />
    <Button fx:id="btnBrowse" mnemonicParsing="false" onAction="#browseAction" text="Carica file" />
    <Button fx:id="btnPreview" mnemonicParsing="false" onAction="#previewAction" text="Ascolta" />
    <Button fx:id="btnElimina" mnemonicParsing="false" onAction="#eliminaAction" text="Elimina" />
  </children>
</fx:root>
